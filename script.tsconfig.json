{
    "compilerOptions": {
        "outDir": "./scripts/",
        "sourceMap": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowJs": true
    },
    "include": [
        "./scripts/**/*.ts",
        "./scripts/typings.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}
